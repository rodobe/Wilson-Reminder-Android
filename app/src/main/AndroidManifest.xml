<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jesus.wilsonreminder2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/elwilson"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/elwilson"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainInicio"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegistroUsuario"
            android:label="@string/title_activity_registro_usuario"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SaludoWilson"
            android:label="@string/title_activity_saludo_wilson"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>